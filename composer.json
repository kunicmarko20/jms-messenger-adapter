{
    "name": "kunicmarko/jms-messenger-adapter",
    "type": "symfony-bundle",
    "description": "Use JMS Serializer with Symfony Messenger.",
    "keywords": [
        "symfony",
        "messenger",
        "serializer",
        "jms",
        "adapter",
        "bundle"
    ],
    "homepage": "https://github.com/kunicmarko20/jms-messenger-adapter",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Kunic",
            "email": "kunicmarko20@gmail.com",
            "homepage": "https://twitter.com/kunicmarko20"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "jms/serializer": "^2.3 || ^3.9",
        "symfony/messenger": "^4.4 || ^5.3 || ^6.0"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "behat/symfony2-extension": "^2.1",
        "jms/serializer-bundle": "^3.9",
        "matthiasnoback/symfony-dependency-injection-test": "dev-master",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "symfony/config": "^4.4 || ^5.3 || ^6.0",
        "symfony/debug": "^4.4 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.3 || ^6.0"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "KunicMarko\\JMSMessengerAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KunicMarko\\JMSMessengerAdapter\\Tests\\": "tests/",
            "KunicMarko\\JMSMessengerAdapter\\Features\\Fixtures\\Project\\": "features/Fixtures/Project/src",
            "KunicMarko\\JMSMessengerAdapter\\Features\\": "features/"
        }
    }
}
